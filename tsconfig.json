{
  "files": [],
  "references": [
    { "path": "./tsconfig.app.json" },
    { "path": "./tsconfig.node.json" }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": "@/*": ["./src/*"]  },
    "jsx": "react-jsx",
    "moduleResolution": "Bundler",
    "target": "ES2020",
    "lib": ["ES2020","DOM", "DOM.Iterable"],
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "allowJs": true,
    "noUnusedLocals": false,
    "strictNullChecks": false
  },
"include": ["src"]
}
